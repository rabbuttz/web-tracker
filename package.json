{
  "name": "webcam-tracker-4-resonite",
  "productName": "WebCamTracker4Resonite",
  "private": true,
  "version": "1.0.0",
  "description": "MediaPipe-based hand and face tracking application with OSC output",
  "author": "Rabbuttz",
  "main": "electron-main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "start": "concurrently -k \"node server.js\" \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron:dev\"",
    "build": "tsc -b && vite build",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.22",
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@mediapipe/hands": "^0.4.1675469240",
    "auto-launch": "^5.0.6",
    "express": "^4.21.2",
    "gl-matrix": "^3.4.4",
    "node-osc": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-webcam": "^7.2.0",
    "three": "^0.164.1",
    "uuid": "^11.0.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^33.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^7.2.0"
  },
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321",
  "build": {
    "appId": "com.rabbuttz.webcamtracker4resonite",
    "productName": "WebCamTracker4Resonite",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "preload.js",
      "server.js",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "build/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "portable": {
      "artifactName": "${productName}-${version}-portable.exe"
    }
  }
}